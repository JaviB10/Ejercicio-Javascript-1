<div>
    <h1>Productos</h1>
    {{#each this.payload}}
        <ul>
            <li>title: {{this.title}}</li> 
            <li>description: {{this.description}}</li>
            <li>code: {{this.code}}</li>
            <li>price: {{this.price}}</li>
            <li>status: {{this.status}}</li>
            <li>stock: {{this.stock}}</li>
            <li>category: {{this.category}}</li>
            <li>id: {{this._id}}</li>
        </ul>
    {{/each}}
    {{#if this.hasPrevPage}}
    <a href={{this.prevLink}}><button>Anterior</button></a>
    {{/if}}

    {{#if this.hasNextPage}}
    <a href={{this.nextLink}}><button>Siguiente</button></a>
    {{/if}}
</div>

